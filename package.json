{
  "name": "nodejs-shtube",
  "version": "1.0.0",
  "description": "Cloning Youtube with nodejs",
  "main": "init.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --ignore 'scss' --ignore 'static'",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "prestart": "npm run build",
    "start": "node build/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh91c/nodejs-shTube.git"
  },
  "author": "sh91c",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sh91c/nodejs-shTube/issues"
  },
  "homepage": "https://github.com/sh91c/nodejs-shTube#readme",
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^10.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.2.1",
    "body-parser": "^1.19.0",
    "bower": "^1.8.8",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.5",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.15.6",
    "helmet": "^4.2.0",
    "mini-css-extract-plugin": "^0.11.2",
    "mongoose": "^5.10.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-sass": "^4.14.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "postcss": "^8.0.5",
    "postcss-loader": "^4.0.2",
    "pug": "^3.0.0",
    "sass-loader": "^10.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "browserslist": [
    "defaults"
  ]
}